#include <stdio.h>

int main() {
    int n, sum = 0;
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    for (int i = 1; i <= n; i += 2) {
        sum += i;
    }
    
    printf("The sum of all odd numbers from 1 to %d is %d", n, sum);
    
    return 0;
}
outout
Enter the value of n: 1
The sum of all odd numbers from 1 to 1 is 1
